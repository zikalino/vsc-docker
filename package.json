{
  "name": "vsc-docker",
  "displayName": "Docker Runner",
  "description": "SmarterCode Docker Tools",
  "version": "0.1.0",
  "repository": "https://github.com/zikalino/vsc-docker",
  "publisher": "Zim",
  "icon": "media/icon.webp",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "docker",
    "azure",
    "cli"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-cloud.displayDockerExplorer",
        "title": "DockerRunner: Tree"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "validate": "node ./out/validate.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "esbuild": "^0.16.17",
    "esbuild-plugin-yaml": "^0.0.1",
    "eslint": "^8.54.0",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@vscode/codicons": "0.0.20",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "@zim.kalinowski/vscode-helper-toolkit": "^0.1.3",
    "jsonpath-plus": "^9.0.0",
    "marked": "^12.0.1",
    "yaml": "^2.3.4"
  }
}
